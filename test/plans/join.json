{
    "id": "01",
    "name": "joiner",
    "tools": [
        {
            "id": "01",
            "tool": "input",
            "path": "./test/data/csv/cars-with-header.csv",
            "limit": 50,
            "format": "csv",
            "header": true
        },
        { 
            "id": "02",
            "tool": "input",
            "path": "./test/data/csv/cars-no-header.csv",
            "format": "csv",
            "header": false,
            "schema": [
                { "Car": "utf8" },
                { "MPG": "f64" },
                { "Cylinders": "i16" },
                { "Displacement": "f32" },
                { "Horsepower": "f32" },
                { "Weight": "f32" },
                { "Acceleration": "f32" },
                { "Model": "i16" },
                { "Origin": "utf8" }
            ]
        },
        { 
            "id": "03",
            "tool": "select",
            "columns": ["Car", "MPG", "Cylinders", "Displacement", "Horsepower"],
            "aliases": {"Car": "Car(L)"}
        },
        { 
            "id": "04",
            "tool": "select",
            "columns": ["Car", "Weight", "Acceleration", "Model", "Origin"],
            "aliases": {"Car": "Car(R)"}
        },
        { "id": "05", "tool": "join", "type": "inner", "lt": ["Car(L)"], "rt": ["Car(R)"] },
        { "id": "06", "tool": "output", "format": "csv", "path": "./output/select", "overwrite": true },
        { "id": "07", "tool": "union", "distinct": true },
        { "id": "08", "tool": "output", "format": "json", "path": "./output/union", "overwrite": true },
        { "id": "09", "tool": "output", "format": "json", "path": "./output/limit", "overwrite": true }
    ],
    "links": [
        { "src": "01", "dst": "03" },
        { "src": "02", "dst": "04" },
        { "src": "03", "dst": "05", "input": "left" },
        { "src": "04", "dst": "05", "input": "right" },
        { "src": "05", "dst": "06" },
        { "src": "01", "dst": "07", "input": "left" },
        { "src": "02", "dst": "07", "input": "right" },
        { "src": "07", "dst": "08" },
        { "src": "01", "dst": "09" }
    ]
}
