{
    "id": "01",
    "name": "joiner",
    "tools": [
        {
            "id": "01",
            "tool": "import",
            "path": "./test/data/csv/cars-with-header.csv",
            "limit": 50,
            "format": "csv",
            "header": true
        },
        { 
            "id": "02",
            "tool": "import",
            "path": "./test/data/csv/cars-no-header.csv",
            "format": "csv",
            "header": false,
            "schema": [
                { "column": "Car", "type": "utf8" },
                { "column": "MPG", "type": "f64" },
                { "column": "Cylinders", "type": "i16" },
                { "column": "Displacement", "type": "f32" },
                { "column": "Horsepower", "type": "f32" },
                { "column": "Weight", "type": "f32" },
                { "column": "Acceleration", "type": "f32" },
                { "column": "Model", "type": "i16" },
                { "column": "Origin", "type": "utf8" }
            ]
        },
        { 
            "id": "03",
            "tool": "select",
            "columns": ["Car", "MPG", "Cylinders", "Displacement", "Horsepower"],
            "aliases": {"Car": "Car(L)"}
        },
        { 
            "id": "04",
            "tool": "select",
            "columns": ["Car", "Weight", "Acceleration", "Model", "Origin"],
            "aliases": {"Car": "Car(R)"}
        },
        { "id": "05", "tool": "join", "type": "inner", "lt": ["Car(L)"], "rt": ["Car(R)"] },
        { "id": "06", "tool": "export", "format": "csv", "path": "./output/select", "overwrite": true },

        { "id": "07", "tool": "union", "distinct": true },
        { "id": "08", "tool": "export", "format": "json", "path": "./output/union", "overwrite": true },
        { "id": "09", "tool": "export", "format": "json", "path": "./output/limit", "overwrite": true },

        {
            "id": "10",
            "tool": "import",
            "path": "./test/data/csv/cars-with-header.csv",
            "format": "csv",
            "header": true,
            "sql": {
                "stmt": "SELECT \"Car\", \"Displacement\", \"Horsepower\", \"Origin\" FROM cars WHERE \"Displacement\" > 390.0",
                "table": "cars"
            }
        },
        { "id": "11", "tool": "export", "format": "csv", "path": "./output/sql", "overwrite": true },
        {
            "id": "12",
            "tool": "import",
            "path": "./test/data/csv/cars-with-header.csv",
            "format": "csv",
            "header": true
        },
        { "id": "13", "tool": "filter", "expr": {"and": [
            {"lte":[{"col": "Horsepower"}, {"f32": 200.0}]},
            {"gt":[{"col": "Displacement"}, {"f32": 390.0}]}
        ]}},
        { "id": "14", "tool": "export", "format": "csv", "path": "./output/filter", "overwrite": true }
    ],
    "links": [
        { "src": "01", "dst": "03" },
        { "src": "02", "dst": "04" },
        { "src": "03", "dst": "05", "input": "left" },
        { "src": "04", "dst": "05", "input": "right" },
        { "src": "05", "dst": "06" },
        { "src": "01", "dst": "07", "input": "left" },
        { "src": "02", "dst": "07", "input": "right" },
        { "src": "07", "dst": "08" },
        { "src": "01", "dst": "09" },
        { "src": "10", "dst": "11" },
        { "src": "12", "dst": "13" },
        { "src": "13", "dst": "14" }
    ]
}
